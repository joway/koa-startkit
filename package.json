{
  "name": "koa-startkit",
  "version": "1.0.0",
  "description": "get start for koa",
  "main": "src/main.js",
  "scripts": {
    "start": "nodemon src/main.js --exec babel-node",
    "prod": "npm run build && node dist/main.js",
    "build": "babel src -d dist",
    "lint": "eslint src/*",
    "clean": "rm -r dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joway/koa-startkit.git"
  },
  "author": "Joway Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joway/koa-startkit/issues"
  },
  "homepage": "https://github.com/joway/koa-startkit#readme",
  "dependencies": {
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.2.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.11.0"
  }
}